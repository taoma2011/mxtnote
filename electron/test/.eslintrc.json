{
 "parserOptions": {
        "ecmaVersion": 8,
         "sourceType": "module"
    },
  "extends": "plugin:testcafe/recommended",
  "env": {

    "jest/globals": true
  },
  "plugins": ["jest", "testcafe", "mocha"],
  "rules": {
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error"
  }
}
